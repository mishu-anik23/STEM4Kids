{
  "name": "stem-game-backend",
  "version": "1.0.0",
  "description": "Backend API for STEM Learning Game",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "node database/migrate.js",
    "seed": "node database/seed.js",
    "seed:physics": "node scripts/seedPhysicsIsland.js",
    "verify:physics": "node scripts/verifyPhysicsData.js",
    "test:api": "node scripts/restartAndTest.js",
    "test": "jest --coverage",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "start:script": "powershell -ExecutionPolicy Bypass -File scripts/start-server.ps1"
